[
    {
        "class": "com.mysql.cj.jdbc.Driver",
        "url": "jdbc:mysql://{Host}:{Port}/{Database}",
        "type": "MySQL",
        "version": "8"
    },
    {
        "class": "org.sqlite.SQLiteDataSource",
        "type": "SQLite",
        "properties": {
            "dataSource.encoding": "UTF-8",
            "dataSource.url": "Data Source={Path}/storage/database.db"
        }
    }
]